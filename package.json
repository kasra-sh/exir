{
  "name": "xeer-js",
  "version": "0.7.1",
  "description": "Javascript Framework",
  "author": "Kasra Shamsaei",
  "license": "MIT",
  "scripts": {
    "generate": "node generateExt.js",
    "bundle": "node generateExt.js && parcel build xeer-bundle.js --target modern && parcel build xeer-bundle-es5.js --target legacy",
    "devServe": "parcel xeer-bundle.js"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "kenpo-js": "0.1.7",
    "parcel": "2.0.0-beta.1",
    "regenerator-runtime": "^0.13.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kasra-sh/xeer-js.git"
  },
  "bugs": {
    "url": "https://github.com/kasra-sh/xeer-js/issues"
  },
  "homepage": "https://github.com/kasra-sh/xeer-js#readme",
  "keywords": [
    "xeer",
    "xeer-js",
    "framework",
    "stream",
    "dom",
    "virtualdom",
    "render",
    "viewmodel",
    "rest",
    "ajax"
  ],
  "directories": {
    "example": "examples"
  },
  "modern": "dist/xeer-bundle.js",
  "legacy": "dist/xeer-bundle-es5.js",
  "targets": {
    "legacy": {
      "engines": {
        "browsers": [
          "ie >= 9",
          "iOS >= 6"
        ]
      }
    },
    "modern": {
      "engines": {
        "browsers": [
          "since 2019"
        ]
      }
    },
    "custom-legacy": {
      "engines": {
        "browsers": [
          "ie >= 9",
          "iOS >= 6"
        ]
      }
    },
    "node": {
      "distDir": "cjs",
      "engines": {
        "node": ">=9.x"
      },
      "context": "node",
      "outputFormat": "commonjs",
      "isLibrary": true
    }
  }
}
