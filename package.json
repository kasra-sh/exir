{
  "name": "exir",
  "version": "0.10.1",
  "description": "Javascript Framework",
  "author": "Kasra Shamsaei",
  "license": "MIT",
  "scripts": {
    "generate": "node generateExt.js",
    "bundle-modern": "parcel build --target modern exir-bundle.js exir-bundle-ext.js",
    "bundle-legacy": "parcel build --target legacy exir-bundle-legacy.js exir-bundle-legacy-ext.js",
    "bundle-node": "parcel build --target node index.js",
    "bundle": "npm run generate && npm run bundle-modern && npm run bundle-legacy",
    "devServe": "parcel exir-bundle-ext.js"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "docdash": "^1.2.0",
    "jsdoc-memberof-namespace": "^2.2.0",
    "kenpo-js": "0.1.7",
    "parcel": "^2.0.0-beta.1",
    "regenerator-runtime": "^0.13.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kasra-sh/exir.git"
  },
  "bugs": {
    "url": "https://github.com/kasra-sh/exir/issues"
  },
  "homepage": "https://github.com/kasra-sh/exir#readme",
  "keywords": [
    "exir",
    "exir-js",
    "xeer",
    "xeer-js",
    "framework",
    "stream",
    "dom",
    "virtualdom",
    "render",
    "viewmodel",
    "rest",
    "ajax"
  ],
  "directories": {
    "example": "examples"
  },
  "targets": {
    "legacy": {
      "engines": {
        "browsers": [
          "ie >= 9",
          "iOS >= 6"
        ]
      }
    },
    "modern": {
      "engines": {
        "browsers": [
          "since 2019"
        ]
      }
    },
    "custom-legacy": {
      "engines": {
        "browsers": [
          "ie >= 9",
          "iOS >= 6"
        ]
      }
    },
    "node": {
      "engines": {
        "node": ">=8.x"
      },
      "context": "node",
      "outputFormat": "commonjs",
      "isLibrary": true
    }
  }
}
