{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": [
      "./core/cases.js",
      "./http"
    ],
    "includePattern": "\\.(vue|js|jsx)$"
  },
  "sourceType": "module",
  "opts": {
    "encoding": "utf8",
    "destination": "/media/blkr/SD64/xeer_docs",
    "recurse": true,
    "readme": "README.md",
    "tutorials": "./tutorials",
    "template": "node_modules/docdash"
  },
  "templates": {
    "search": true,
    "cleverLinks": true,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true,
      "includeDate": false
    },
    "scripts": [],
    "favicon": "",
    "css": []
  },
  "plugins": [
    "plugins/markdown",
    "node_modules/jsdoc-memberof-namespace"
  ],
  "docdash": {
    "static": true,
    "sort": false,
    "sectionOrder": [
      "Tutorials",
      "Modules",
      "Externals",
      "Events",
      "Classes",
      "Mixins",
      "Interfaces"
    ],
    "disqus": "",
    "openGraph": {
      "title": "",
      "type": "website",
      "image": "",
      "site_name": "",
      "url": ""
    },
    "meta": {
      "title": "XEER JS",
      "description": "Javascript Framework",
      "keyword": ""
    },
    "search": true,
    "collapse": true,
    "typedefs": true,
    "navLevel": 3,
    "scripts": [],
    "menu": {
      "Project Website": {
        "href":"https://github.com/kasra-sh/xeer-js",
        "target":"_blank",
        "class":"menu-item",
        "id":"website_link"
      }
    }
  }
}